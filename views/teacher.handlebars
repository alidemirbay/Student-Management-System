<div class="d-flex" id="wrapper">

    <!-- Sidebar -->
    <div class="bg-light border-right" id="sidebar-wrapper">
        <div class="sidebar-heading">WELCOME <span class="member-name"></span> </div>
        {{>pageparts/aside}}

        <!-- Page Content -->
        <div id="page-content-wrapper">

            {{>pageparts/navbar}}


            <div class="container-fluid">
                <div class="d-flex align-items-center p-3 my-3 text-white-50 bg-purple rounded box-shadow">
                    <div class="lh-100">
                        <h6 class="mb-0 text-white lh-100">Teachers</h6>
                        <small>University of Washington Bootcamp</small>
                    </div>
                </div>

                <div class="my-3 p-3 bg-white rounded box-shadow">
                    <h6 class="border-bottom border-gray pb-2 mb-0"></h6>
                    {{#each data}}
                    {{>pageparts/teacherpart}}
                    {{/each}}
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Menu Toggle Script -->
    <script>
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });
    </script>